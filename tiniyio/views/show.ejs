<%- include('partials/header') %>

<div class="container">
  <canvas id="myChart" width="100" height="50"></canvas>
  <script>
  var ctx = document.getElementById('myChart');
  var data1 = <%= table.roll %>,
      data3 = <%= table.hour %>,
      data4 = <%= table.failed_count %>, 
      data5 = <%= table.timeout_count %>,
      data6 = <%= table.sms_count %>;

  var myChart = new Chart(ctx, {
      type: 'bar',
      data: {
          labels: ['Roll_number', 'hour', 'failed_count', 'timeout_count' ,'sms_count'],
          
          datasets: [{
              label: '# of Votes',
              data: [data1, data3, data4, data5, data6],
              backgroundColor: [
                  'rgba(255, 99, 132,0.8)',
                  'rgba(54, 162, 235,0.8)',
                  'rgba(255, 206, 86,0.8)',
                  'rgba(75, 192, 192,0.8)',
                  'rgba(153, 102, 255,0.8)',
                  'rgba(255, 159, 64, 0.2)'
              ],
              borderColor: [
                  'rgba(255, 99, 132, 1)',
                  'rgba(54, 162, 235, 1)',
                  'rgba(255, 206, 86, 1)',
                  'rgba(75, 192, 192, 1)',
                  'rgba(153, 102, 255, 1)',
                  'rgba(255, 159, 64, 1)'
              ],
              borderWidth: 1
          }]
      },
      options: {
          scales: {
              yAxes: [{
                  ticks: {
                      beginAtZero: true
                  }
              }]
          }
      }
  });
  </script>
</div>

<%- include('partials/footer') %>